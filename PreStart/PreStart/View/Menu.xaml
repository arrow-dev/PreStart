<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Prestart.View.Menu"
             MasterBehavior="Popover">
  <MasterDetailPage.Master>
    <ContentPage Title="Menu" Padding="0,40,0,0">
      <StackLayout>
        <Button Text="Hazard Register / Sign On" Clicked="SignOnButton_OnClicked"/>
        <Button Text="Prestart Log" Clicked="PrestartLogButton_OnClicked"/>
      </StackLayout>
    </ContentPage>
  </MasterDetailPage.Master>
</MasterDetailPage>