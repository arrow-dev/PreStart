<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Prestart.View.Menu"
             MasterBehavior="Popover">
  <MasterDetailPage.Master>
    <ContentPage Title="Menu">
      <StackLayout>
        <Image Source="logo.png" Aspect="AspectFill"></Image>
        <Button Text="Prestart Log" Clicked="PrestartLogButton_OnClicked"/>
        <Button Text="Review / Sign On" Clicked="SignOnButton_OnClicked"/>
      </StackLayout>
    </ContentPage>
  </MasterDetailPage.Master>
</MasterDetailPage>